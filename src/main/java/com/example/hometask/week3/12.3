public class Ex123 {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        boolean b = true;
        int[] arr = new int[100];
        for (int i = 0; i < 100; i++) {
            arr[i] = (int) (Math.random() * 100);
        }
        while (b) {
            try {
                int n = sc.nextInt();
                System.out.println(arr[n]);
                b = false;
            } catch (ArrayIndexOutOfBoundsException e) {
                System.out.println("Out of bounds");
            }
        }
}
}
